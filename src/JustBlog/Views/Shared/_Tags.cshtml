@model IList<JustBlog.Core.Objects.Tag>

<div class="tags sidebar">
    <h3>Tags</h3>

    @if(Model.Count > 0)
    {
        <ul>
            @foreach(var tags in Model)
            {
                <li>@Html.TagLink(tags)</li>
            }
        </ul>
    }
    else
    {
        <p>No Tags Found !</p>
    }
</div>
